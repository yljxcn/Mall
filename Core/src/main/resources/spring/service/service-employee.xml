<?xml version="1.0" encoding="UTF-8"?>

<beans xmlns="http://www.springframework.org/schema/beans" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-3.2.xsd">
  <bean id="employeeModuleService" class="com.xmg.mall.employee.service.impl.BasicEmployeeModuleService">
    <property name="employeeService" ref="employeeService"/>
    <property name="totalEmployeeService" ref="totalEmployeeService"/>
  </bean>
  <bean id="employeeService" class="com.xmg.mall.employee.service.impl.BasicEmployeeService">
    <property name="employeeMapper" ref="employeeMapper"/>
  </bean>
  <bean id="totalEmployeeService" class="com.xmg.mall.employee.service.impl.BasicTotalEmployeeService">
    <property name="totalEmployeeMapper" ref="totalEmployeeMapper"/>
  </bean>
</beans>
